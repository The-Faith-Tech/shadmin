  "version": "0.2.0",
  "configurations": [
    {
      "name": "shadmin",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "shadmin (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "shadmin (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "shadmin_ui",
      "cwd": "example/shadmin_ui",
      "request": "launch",
      "type": "dart",
      "args": ["-d", "chrome", "--web-renderer", "html"]
    },
    {
      "name": "shadmin_ui (profile mode)",
      "cwd": "example/shadmin_ui",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["-d", "chrome", "--web-renderer", "html"]
    },
    {
      "name": "shadmin_ui (release mode)",
      "cwd": "example/shadmin_ui",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["-d", "chrome", "--web-renderer", "html"]
    }
  ]
}
